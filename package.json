{
  "name": "samuraispirits-command",
  "private": true,
  "version": "1.8.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:h": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "preview:h": "vite preview --host",
    "sprite": "npm run icons && npm run covers",
    "icons": "svg-sprite -s --symbol-dest=public --ss=assets/icons.svg --svg-xmldecl=false --shape-id-separator= ./src/assets/icons/*.svg",
    "covers": "svg-sprite -s --symbol-dest=public --ss=assets/covers.svg --svg-xmldecl=false --shape-id-separator= ./src/assets/covers/*.svg",
    "up": "npx npm-check-updates",
    "up:u": "npx npm-check-updates -u"
  },
  "license": "MIT",
  "dependencies": {
    "clsx": "^2.1.1",
    "musubii": "^8.2.0",
    "preact": "^10.25.0",
    "react-router-dom": "^7.0.1"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.1.0",
    "@types/node": "^22.9.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remark-gfm": "^4.0.0",
    "svg-sprite": "^2.0.4",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
  }
}
