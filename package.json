{
  "name": "samuraispirits-command",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:h": "vite --host",
    "build": "npm run build:client && npm run build:server && npm run build:ssg",
    "build:client": "vite build --config vite.config.client.js",
    "build:server": "vite build --config vite.config.server.js",
    "build:ssg": "tsx scripts/ssg.ts",
    "sprite": "tsx scripts/sprite.ts",
    "ssg": "npm run build:server && npm run build:ssg",
    "preview": "vite preview",
    "preview:h": "vite preview --host",
    "up": "npx npm-check-updates",
    "up:u": "npx npm-check-updates -u"
  },
  "license": "MIT",
  "dependencies": {
    "clsx": "^2.1.1",
    "musubii": "^8.3.0",
    "preact": "^10.25.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/svg-sprite": "^0.0.39",
    "@vitejs/plugin-react": "^4.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "svg-sprite": "^2.0.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.1"
  }
}
